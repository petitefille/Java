public class TallvariablerA {
    public static void main(String[] args){
        int four = 2 + 2;
        int twenty = 2 + 3*6;
        double OnePointFive = 3./2.;
        int t1 = 2 * 3 + 4;
        int t2 = 2 - 3 * 4;
        System.out.println("2 + 2 = " + four);
        System.out.println("2 + 3*6 = " + twenty);
        System.out.println("3/2 = " + OnePointFive);
        System.out.println("t1 = " + t1);
        System.out.println("t2 = " + t2);
    }
}       
