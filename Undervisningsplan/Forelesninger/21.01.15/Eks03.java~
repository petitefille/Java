        Katt tom = new Katt("Tom2", pus);

        katteburet.settInn(tom);
        Hund h = hundeburet.taUt();

        Bur<Bil> garasje = new Bur<Bil>();

        Bur<Person> hybel = new Bur<Person>();
    }
}
